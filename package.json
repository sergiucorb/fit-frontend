{
  "name": "fit-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "download-schema": "apollo service:download --endpoint=http://localhost:4000/ graphql-schema.json",
    "codegen": "npm run download-schema && apollo codegen:generate --localSchemaFile=graphql-schema.json,client-schema.graphql --target=typescript --tagName=gql",
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "graphql": "^16.5.0",
    "next": "12.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.6.4",
    "@types/react": "18.0.16",
    "@types/react-dom": "18.0.6",
    "apollo": "^2.34.0",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "typescript": "4.7.4"
  }
}
